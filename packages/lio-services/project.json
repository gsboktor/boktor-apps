{
  "name": "lio-services",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/lio-services/src",
  "projectType": "application",
  "tags": ["scope:services"],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "local",
      "configurations": {
        "local": {
          "optimization": false,
          "sourceMap": true,
          "environment": {
            "NODE_ENV": "local"
          }
        },
        "development": {
          "optimization": false,
          "sourceMap": true,
          "environment": {
            "NODE_ENV": "development"
          }
        },
        "production": {
          "optimization": true,
          "environment": {
            "NODE_ENV": "production"
          }
        }
      },
      "options": {
        "compiler": "tsc",
        "main": "packages/lio-services/src/main.ts",
        "outputPath": "dist/packages/lio-services",
        "webpackConfig": "packages/lio-services/webpack.config.js",
        "sourceMap": false
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "local",
      "dependsOn": ["build"],
      "options": {
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "local": {
          "buildTarget": "lio-services:build:local"
        },
        "development": {
          "buildTarget": "lio-services:build:development"
        },
        "production": {
          "buildTarget": "lio-services:build:production"
        }
      }
    }
  }
}
